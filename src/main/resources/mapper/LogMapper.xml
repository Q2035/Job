<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.hellooooo.job.mapper.LogMapper">

    <insert id="info" parameterType="UserActionInfo">
        insert into j_log_user
        values (default, #{userId}, #{username}, #{message});
    </insert>
</mapper>